<UserControl x:Class="Battle.ui.VarCompList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:l="clr-namespace:Battle.ui"
             mc:Ignorable="d" 
             d:DesignHeight="160.118" d:DesignWidth="311.777">
	<Grid>
		<StackPanel>
			<l:StructNavi x:Name="navi"></l:StructNavi>
			<StackPanel x:Name="stack" Grid.ColumnSpan="3" Grid.IsSharedSizeScope="True"
						Grid.RowSpan="3">
				<StackPanel.Resources>
					<Style TargetType="{x:Type l:VarCoparition}">
						<Setter Property="Margin" Value="0,3,0,0"/>
					</Style>
				</StackPanel.Resources>
				<l:VarCoparition ></l:VarCoparition>
				<l:VarCoparition varName="some"></l:VarCoparition>
			</StackPanel>
		</StackPanel>

	</Grid>
</UserControl>
